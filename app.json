{
  "expo": {
    "name": "beach-finder-app",
    "slug": "beach-finder-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "beach-finder-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.beachfinderapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            {
              "LeagueSpartan-Regular": "node_modules/@expo-google-fonts/league-spartan/static/LeagueSpartan-Regular.ttf"
            },
            {
              "LeagueSpartan-Medium": "node_modules/@expo-google-fonts/league-spartan/static/LeagueSpartan-Medium.ttf"
            },
            {
              "LeagueSpartan-Bold": "node_modules/@expo-google-fonts/league-spartan/static/LeagueSpartan-Bold.ttf"
            }
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
